# GraphQL schema example
#
# https://gqlgen.com/getting-started/
# panggil gqlgen generate abis update ini

type User {
  fullname: String!
  email: String!
  password: String!
}

input UserInput {
  fullname: String!
  email: String!
  password: String!
}

type Mutation {
  CreateUser(input: UserInput!): User!
}

type Query {
  GetAllUsers: [User!]!
}